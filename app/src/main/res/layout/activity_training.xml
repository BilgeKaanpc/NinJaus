<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Training">

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="667dp"
        android:layout_height="790dp"
        android:layout_marginBottom="150dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/arkap" />
    <androidx.gridlayout.widget.GridLayout
        android:id="@+id/gridLayout2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="85dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible">

        <ImageView
            android:id="@+id/m0"
            android:onClick="tik"
            android:layout_width="100dp"
            android:layout_height="130dp"
            app:layout_column="0"
            app:layout_row="0"
            app:srcCompat="@drawable/lj" />

        <ImageView
            android:id="@+id/m1"
            android:onClick="tik"
            android:layout_width="100dp"
            android:layout_height="130dp"
            app:layout_column="1"
            app:layout_row="0"
            app:srcCompat="@drawable/lj" />

        <ImageView
            android:id="@+id/m2"
            android:onClick="tik"
            android:layout_width="100dp"
            android:layout_height="130dp"
            app:layout_column="2"
            app:layout_row="0"
            app:srcCompat="@drawable/rj" />

        <ImageView
            android:id="@+id/m3"
            android:onClick="tik"
            android:layout_width="100dp"
            android:layout_height="130dp"
            app:layout_column="3"
            app:layout_row="0"
            app:srcCompat="@drawable/rj" />

        <ImageView
            android:id="@+id/m4"
            android:onClick="tik"
            android:layout_width="100dp"
            android:layout_height="130dp"
            app:layout_column="0"
            app:layout_row="1"
            app:srcCompat="@drawable/lj" />

        <ImageView
            android:id="@+id/m5"
            android:onClick="tik"
            android:layout_width="100dp"
            android:layout_height="130dp"
            app:layout_column="1"
            app:layout_row="1"
            app:srcCompat="@drawable/lj" />

        <ImageView
            android:id="@+id/m6"
            android:onClick="tik"
            android:layout_width="100dp"
            android:layout_height="130dp"
            app:layout_column="2"
            app:layout_row="1"
            app:srcCompat="@drawable/rj" />

        <ImageView
            android:id="@+id/m7"
            android:onClick="tik"
            android:layout_width="100dp"
            android:layout_height="130dp"
            app:layout_column="3"
            app:layout_row="1"
            app:srcCompat="@drawable/rj" />

        <ImageView
            android:id="@+id/m8"
            android:onClick="tik"
            android:layout_width="100dp"
            android:layout_height="130dp"
            app:layout_column="0"
            app:layout_row="2"
            app:srcCompat="@drawable/lj" />

        <ImageView
            android:id="@+id/m9"
            android:onClick="tik"
            android:layout_width="100dp"
            android:layout_height="130dp"
            app:layout_column="1"
            app:layout_row="2"
            app:srcCompat="@drawable/lj" />

        <ImageView
            android:id="@+id/m10"
            android:onClick="tik"
            android:layout_width="100dp"
            android:layout_height="130dp"
            app:layout_column="2"
            app:layout_row="2"
            app:srcCompat="@drawable/rj" />

        <ImageView
            android:id="@+id/m11"
            android:onClick="tik"
            android:layout_width="100dp"
            android:layout_height="130dp"
            app:layout_column="3"
            app:layout_row="2"
            app:srcCompat="@drawable/rj" />

        <ImageView
            android:id="@+id/m12"
            android:onClick="tik"
            android:layout_width="100dp"
            android:layout_height="130dp"
            app:layout_column="0"
            app:layout_row="3"
            app:srcCompat="@drawable/lj" />

        <ImageView
            android:id="@+id/m13"
            android:onClick="tik"
            android:layout_width="100dp"
            android:layout_height="130dp"
            app:layout_column="1"
            app:layout_row="3"
            app:srcCompat="@drawable/lj" />

        <ImageView
            android:id="@+id/m14"
            android:onClick="tik"
            android:layout_width="100dp"
            android:layout_height="130dp"
            app:layout_column="2"
            app:layout_row="3"
            app:srcCompat="@drawable/rj" />

        <ImageView
            android:id="@+id/m15"
            android:onClick="tik"
            android:layout_width="100dp"
            android:layout_height="130dp"
            app:layout_column="3"
            app:layout_row="3"
            app:srcCompat="@drawable/rj" />

    </androidx.gridlayout.widget.GridLayout>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="100dp"
        android:layout_height="36dp"
        android:layout_marginEnd="300dp"
        android:text="Time"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gridLayout2" />

    <TextView
        android:id="@+id/time"
        android:layout_width="100dp"
        android:layout_height="36dp"
        android:layout_marginEnd="300dp"
        android:text="30"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="97dp"
        android:layout_height="45dp"
        android:layout_marginStart="307dp"
        android:layout_marginEnd="10dp"
        android:text="Score"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/point"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gridLayout2" />

    <TextView
        android:id="@+id/point"
        android:layout_width="103dp"
        android:layout_height="43dp"
        android:layout_marginStart="300dp"
        android:text="0"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.7"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="91dp"
        android:layout_height="35dp"
        android:layout_marginBottom="5dp"
        android:text="Best Score"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/top"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gridLayout2" />

    <TextView
        android:id="@+id/top"
        android:layout_width="90dp"
        android:layout_height="32dp"
        android:layout_marginBottom="10dp"
        android:text="0"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/point"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>