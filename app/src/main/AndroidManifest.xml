<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bilge.catchminato"
    android:versionCode="5"
    android:versionName="1.4">

    <application
        android:allowBackup="true"
        android:icon="@drawable/gamepp"
        android:label="Nin Jaus"
        android:roundIcon="@drawable/gamepp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-1056663813267203~9862545014"/>
        <activity android:name=".message"></activity>
        <activity android:name=".game" />
        <activity android:name=".levels" />
        <activity android:name=".Shop" />
        <activity android:name=".Training" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
